<div id="main-content"> <!-- Main Content Section with everything -->	
<div class="content-box"><!-- Start Content Box -->
<div class="content-box-content">
<div class="tab-content default-tab" id="tab2"> <!-- This is the target div. id must match the href of this div's tab -->
				
				<table id="table" >
							<thead>
								<tr>
								<th>镜像名称</th>
								<th>镜像大小</th>
								 <th>镜像格式</th>
								 <th>镜像路径</th>
								  <th>操作</th>
								</tr>
							</thead>
					
							<tfoot>
								<tr>
									<td colspan="5">

									</td>
								</tr>
							</tfoot>
						 
							<tbody >
								#foreach($file in $!fileList)
								  #if($!{velocityCount}%2!=0)
								<tr class="alt-row">
								#else
								<tr>
								#end
									<td>$!file.fileName</td>
									<td>$!{file.fileSize}MB</td>
									<td>$!file.fileFormat</td>
									<td>$!filePath</td>
									<td>
										 <a class="addImage" fileName="$!file.fileName">
										 <img src="/resources/images/icons/add_image.gif" alt="添加为模板" title="添加为模板">
                                         </a> 
									</td>
								</tr>
								#end
							</tbody>
						</table>
						
        
                  
					</div> <!-- End #tab2 -->    
				</div> <!-- End .content-box-content -->
			</div> <!-- End .content-box -->		
</div> <!-- End #main-content -->

